<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.patdugan.usmcprofitness"
    android:versionCode="7"
    android:versionName="1.1.3" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name="org.holoeverywhere.app.Application"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Holo.Theme.Light" >
        <activity
            android:name="com.patdugan.usmcprofitness.USMCProFitMainActivity"
            android:label="@string/app_name"
            android:logo="@drawable/logo_simple" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.patdugan.usmcprofitness.AddTestScoreMainActivity"
            android:label="@string/add_score_main_title"
            android:logo="@drawable/logo_simple" >
        </activity>
        <activity
            android:name="com.patdugan.usmcprofitness.AboutActivity"
            android:label="@string/about_title"
            android:logo="@drawable/logo_simple" >
        </activity>
        <activity
            android:name="com.patdugan.usmcprofitness.AddTestScorePFTActivity"
            android:label="@string/add_score_pft_title"
            android:logo="@drawable/logo_simple" >
        </activity>
        <activity
            android:name="com.patdugan.usmcprofitness.CreateProfileActivity"
            android:label="@string/manage_profile_title"
            android:logo="@drawable/logo_simple" >
        </activity>
        <activity
            android:name="com.patdugan.usmcprofitness.ViewTestScoresActivity"
            android:label="@string/view_score_title"
            android:logo="@drawable/logo_simple" >
        </activity>
        <activity
            android:name="com.patdugan.usmcprofitness.ViewDetailedTestScoreActivity"
            android:label="@string/view_score_detailed_title"
            android:logo="@drawable/logo_simple" >
        </activity>
    </application>

</manifest>